{
  "name": "c3-element-plus",
  "version": "1.4.1",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-plugin-vue": "^7.12.0",
    "husky": "^6.0.0",
    "lerna": "^3.22.1",
    "lint-staged": ">=10",
    "ora": "^5.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^10",
    "standard-version": "^9.3.0",
    "vue-dotmd-loader": "^0.2.2",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^4.7.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "gen": "bash ./script/gc.sh",
    "build": "yarn clean:lib && yarn build:comp && yarn build:lib && yarn build:theme",
    "build:comp":"node ./build/bincomp.js",
    "build:theme": "rimraf packages/theme-chalk/lib && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk && rimraf packages/theme-chalk/lib",
    "build:lib": "cross-env LIBMODE=core webpack --config ./build/webpack.config.js",
    "bootstrap": "yarn --frozen-lockfile && npx lerna bootstrap",
    "prepare": "husky install",
    "release": "yarn build && bash ./script/release.sh",
    "clean:lib": "rimraf lib",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "element-ui": "2.12.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  }
}
